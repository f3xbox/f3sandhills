{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "required": ["workouts"],
  "properties": {
    "workouts": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["ao", "style", "location", "day", "time"],
        "properties": {
          "ao": {
            "type": "string"
          },
          "style": {
            "type": "string",
            "enum": [
              "Murph",
              "Beatdown",
              "Run",
              "Trail Run",
              "Ruck",
              "Rucks & Sandbags",
              "Weight Lifting",
              "3rd F (Faith)"
            ]
          },
          "location": {
            "type": "object",
            "required": ["href", "text"],
            "properties": {
              "href": {
                "type": "string",
                "format": "uri"
              },
              "text": {
                "type": "string"
              }
            }
          },
          "day": {
            "type": "string",
            "enum": [
              "Monday",
              "Tuesday",
              "Wednesday",
              "Wednesday Night",
              "Thursday",
              "Thursday Night",
              "Friday",
              "Saturday"
            ]
          },
          "time": {
            "type": "string",
            "enum": [
              "4:45 AM - 5:30 AM",
              "5:30 AM - 6:15 AM",
              "6:30 AM - 7:30 AM",
              "6:00 AM - 6:45 PM"
            ]
          }
        }
      }
    }
  }
}
